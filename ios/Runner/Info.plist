<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>SOS Identity</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>safe_id_app</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>This app uses your location to include a GPS link in SOS alerts.</string>
	    <!-- Location permission: used for GPS link in SOS messages -->
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>
      SOS Identity uses your location only when you choose to include a GPS link in an SOS alert to your trusted contacts.
    </string>

    <!-- Camera permission: used for taking photos of IDs -->
    <key>NSCameraUsageDescription</key>
    <string>
      SOS Identity uses your camera so you can capture photos of your ID documents for quick reference in emergencies.
    </string>

    <!-- Photo library permission: used for picking existing ID photos -->
    <key>NSPhotoLibraryUsageDescription</key>
    <string>
      SOS Identity needs access to your photos so you can select existing images of your IDs to store in the app.
    </string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>This app uses location data to support identity verification and security-related features when required.</string>

	<key>NSLocationWhenInUseUsageDescription</key>
	<string>SOS Identity uses your location only when you choose to include a GPS link in an SOS alert to your trusted contacts.</string>

	<key>NSLocationAlwaysUsageDescription</key>
	<string>This app may use location data in the background to support security-related functionality when enabled to only trusted contacts.</string>


</dict>
</plist>
